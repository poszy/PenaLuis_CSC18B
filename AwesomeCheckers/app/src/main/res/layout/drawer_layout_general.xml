<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dl_drawer_layout_general"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/rl_inclusion_space"
        tools:context="poszmod.awesome">

        <include android:id="@+id/toolbar_custom" layout="@layout/app_bar" />

    </RelativeLayout>

    <fragment
        android:id="@+id/fragment_navigation_drawer"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_gravity="start"
        app:layout="@layout/fragment_navigation_general"
        android:name="poszmod.awesome.NavigationFragmentGeneral"
        tools:layout="@layout/fragment_navigation_general"/>

</android.support.v4.widget.DrawerLayout>